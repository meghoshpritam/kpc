{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-undef": "error",
    "prettier/prettier": ["error"],
    "no-unused-labels": "error",
    "no-unused-expressions": "error",
    "no-unused-vars": "error",
    "no-underscore-dangle": "off",
    "no-loop-func": "off",
    "no-restricted-syntax": "off"
  }
}
